- title = 'Import Dashboard'
- content_for :title, title
%h1= content_for(:title)
= render partial: 'menus/admin_tabs'


.row
  .col-sm-4
    = render 'import_status'
  .col-sm-4
    = render 'match_status' if can_edit_clients?
  .col-sm-4
    = render 'service_status' if can_view_imports?

= link_to 'Debugging Dashboard', admin_dashboard_debug_index_path if can_view_imports?
