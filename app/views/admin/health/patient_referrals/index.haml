= render 'header'
#notice
= render 'tabs'
- if @pagy.count.positive?
  .d-flex{ class: @pagy.pages > 1 ? '' : 'mb-4'}
    = render 'paginate'
    - if can_administer_health?
      = render 'actions'
  = render 'patient_referrals'
  .mt-4!= pagy_bootstrap_nav(@pagy) if @pagy.pages > 1
  = render 'assign_agency_js'
  = render 'reject_js'
- else
  .none-found No patients found
