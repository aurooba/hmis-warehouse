= simple_form_for @filter, as: :filter, url: client_hud_lots_path, method: :get do |f|
  - content_for :filters_col_full do
    .row
      .col-md-6
        = f.input :end, as: :date_picker, label: 'Report Date'

  - content_for :filter_actions do
    = f.button :submit, value: 'Set Date'

  = render 'warehouse_reports/filters', f:f, submit_button: nil
