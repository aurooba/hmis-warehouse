wb = xlsx_package.workbook
wb.add_worksheet(name: "Exit Detail #{GrdaWarehouse::WarehouseReports::Dashboard::Base.available_sub_populations.invert[@filter.sub_population]}"[0..30]) do |sheet|
  title = sheet.styles.add_style(sz: 12, b: true, alignment: {horizontal: :center})
  sheet.add_row([
    'Warehouse Client ID',
    'First Name',
    'Last Name',
    'Date',
    'Destination',
    'Exit From',
    'Ethnicity',
  ] + GrdaWarehouse::Hud::Client.race_fields.map { |m| HUD.race(m) }, style: title)
  @clients.each do |client|
    row = [
      client[:client_id],
      client[:first_name],
      client[:last_name],
      client[:date],
      client[:destination],
      client[:project_name],
      HUD.ethnicity(client[:ethnicity]),
    ] + GrdaWarehouse::Hud::Client.race_fields.map { |m| HUD.no_yes_missing(client[m.to_sym]) }
    sheet.add_row(row)
  end
end
