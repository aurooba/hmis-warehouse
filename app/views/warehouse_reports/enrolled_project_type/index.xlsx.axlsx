wb = xlsx_package.workbook
wb.add_worksheet(name: "Enrolled Clients") do |sheet|
  title = sheet.styles.add_style(sz: 12, b: true, alignment: {horizontal: :center})
  sheet.add_row([
   "Clients enrolled in #{@filter.project_type_codes.join(', ').upcase} between #{@filter.start} and #{@filter.end}"
  ])
  sheet.merge_cells("A1:F1")
  sheet.add_row([
    'Client ID',
    'First Name',
    'Last Name',
    'DOB',
    'Veteran Status',
    "Cumulative Days Homeless as of #{Date.current}",
    "Days Homeless in The Past Three Years as of #{Date.current}",
    'Ethnicity',
  ] + GrdaWarehouse::Hud::Client.race_fields.map { |m| HUD.race(m) })
  @clients.each do |client|
    sheet.add_row([
      client.id,
      client.FirstName,
      client.LastName,
      client.DOB,
      HUD.veteran_status(client.VeteranStatus),
      client.days_homeless,
      client.days_homeless_in_last_three_years,
      HUD.ethnicity(client.Ethnicity),
    ] + client.attributes.slice(*GrdaWarehouse::Hud::Client.race_fields).values.map { |m| HUD.no_yes_missing(m) })
  end
end
