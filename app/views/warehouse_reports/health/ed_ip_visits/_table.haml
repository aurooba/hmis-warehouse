.card
  %table.table.table-striped
    %thead
      %tr
        %th Type
        %th Upload Date
        %th Status
        %th
    %tbody
      - @files.each do |file|
        %tr
          %td= link_to file.label, warehouse_reports_health_ed_ip_visit_path(file)
          %td= file.created_at
          %td= file.status
          %td
            = link_to warehouse_reports_health_ed_ip_visit_path(file), method: :delete, class: 'btn btn-danger btn-sm btn-icon-only', data: {confirm: "Are you sure you want to delete this file?"} do
              %i.icon-cross
