- title = 'Incorrect Move-in Dates'
- content_for :title, title
%h1= title
= render '/warehouse_reports/breadcrumbs', tab: 'data-quality'
= render 'filter'

- if @pagy.count.positive?
  .text-right.mb-4
    = link_to({format: :xlsx}.merge(link_params),{class: 'btn btn-secondary'}) do
      %i.icon-file-excel.icon-lg
      Download
  = render 'common/pagination_top', item_name: 'enrollment'
  .table-responsive
    %table.table.table-striped
      %thead
        %tr
          %th Client
          %th Project
          %th Entry Date
          %th Move-in Date
          %th Exit Date
          %th Destination
      %tbody
        - @enrollments.each do |en|
          - client = en.client.destination_client
          - project = en.project
          %tr
            %td= link_to_if can_view_clients?, client.name, appropriate_client_path(client)
            %td= project.name(current_user)
            %td= en.EntryDate
            %td= en.MoveInDate
            %td= en.exit&.ExitDate
            %td= HUD.destination(en.exit&.Destination)
  = render 'common/pagination_bottom', item_name: 'enrollment'
- else
  .none-found.mt-4 No enrollments found.
