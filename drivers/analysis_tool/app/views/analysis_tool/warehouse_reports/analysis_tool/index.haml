:ruby
  @layout__plain_inner = true
  @layout__width = 'lg'

- title = _('Analysis Tool')
- content_for :title, title
.content-outer-prs1
  %nav.nav-drawer.prs1
    = render 'filters/filter_controls/sidebar'
  .main-outer-prs1
    .utility-prs1
      = render '/warehouse_reports/breadcrumbs', tab: 'operational'
      = render 'utility'
    .top-nav-prs1
      %h1.mb-5= title
    .main-inner-prs1
      .mt-2= render 'disaggregation_picker'
      = render 'table'
