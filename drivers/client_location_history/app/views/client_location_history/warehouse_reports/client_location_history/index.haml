- title = _('Client Contact Locations')
- content_for :title, title

= render '/warehouse_reports/breadcrumbs', tab: 'operational'
%h1.mb-5= title

= render 'filter'

- if @contacts.any?
  #client__location-history
  = content_for :page_js do
    :coffeescript
      markers = #{@markers.to_json.html_safe}
      options = #{@options.to_json.html_safe}
      map = new App.Maps.MapWithMarkers('client__location-history', markers, options)
- else
  .no-data
    No previous locations
