- title = _('Core Demographics')
- content_for :title, title

- @layout__plain_inner = true
- @layout__width = 'lg'
.content-outer-prs1
  %nav.nav-drawer.prs1
    = render 'filters/filter_controls/sidebar'
  .main-outer-prs1
    .utility-prs1
      = render '/warehouse_reports/breadcrumbs', tab: 'operational'
    .top-nav-prs1
      %h1.mb-5= title
      .mb-4= render 'hero_counts'
    .main-inner-prs1
      = render 'ages'
      .row
        .col-sm-6
          = render 'genders'
          = render 'races'
          = render 'ethnicities'
          = render 'disabilities'
          = render 'relationships'
          = render 'dvs'
          = render 'priors'
        .col-sm-6
          = render 'gender_ages'
