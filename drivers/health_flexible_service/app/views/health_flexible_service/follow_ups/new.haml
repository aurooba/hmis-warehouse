= render 'breadcrumbs'
- title = 'Add A Flexible Service VPR Follow Up Form'
= content_for(:title, title)
.o-page
  .o-page__header
    %h1= "#{content_for(:title)} for #{@client.full_name}"
  .o-page__content
    = simple_form_for @follow_up, as: :follow_up, url: client_health_flexible_service_vpr_follow_ups_path(@client) do |f|
      = render 'form', f: f
      = f.button :submit, value: 'Add Follow Up'
