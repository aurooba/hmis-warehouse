- if @html
  = render 'breadcrumbs'
  - title = "Flexible Service VPR Follow Up Form for #{@client.full_name}"
  - content_for(:title, title)
  .o-page
    .o-page__header
      %h1= content_for(:title)
    .o-page__content
      = simple_form_for @follow_up, as: :follow_up, url: client_health_flexible_service_vpr_follow_up_path(@client, @follow_up), wrapper: :readonly do |f|
        = render 'form', f: f
- else
  = render 'show_pdf'
