          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "3.917-A",
              "operator": "NOT_EQUAL",
              "answer_boolean": false
            },
            {
              "question": "3.917-B",
              "operator": "NOT_EQUAL",
              "answer_boolean": false
            }
          ]
        },
        {
          "type": "CHOICE",
          "link_id": "3.917-5",
