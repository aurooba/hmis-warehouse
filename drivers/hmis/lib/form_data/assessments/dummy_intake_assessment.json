    {
      "type": "GROUP",
      "link_id": "3.917.group",
      "text": "Prior Living Situation (3.917)",
      "data_collected_about": "HOH_AND_ADULTS",
      "reference_link": "https://files.hudexchange.info/resources/documents/FY-2022-HMIS-Data-Standards-Manual.pdf#page=104",
      "item": [
        {
          "type": "DISPLAY",
          "link_id": "3.917.instruction",
          "text": "Where did the client spend last night? Select the category that most closely matches the client's response."
        },
        {
          "type": "CHOICE",
          "required": false,
          "link_id": "3.917.1",
          "text": "Prior Living Situation",
          "pick_list_reference": "PRIOR_LIVING_SITUATION"
        },
        {
          "type": "CHOICE",
          "link_id": "3.917.2",
          "text": "Length of stay in prior living situation",
          "pick_list_reference": "LengthOfStay"
        },
        {
          "type": "DISPLAY",
          "link_id": "3.917.A",
          "read_only": true,
          "text": "Client stayed 90+ days in an institutional setting. This is considered a \"break\" according to the HUD definition of chronic homelessness. Stopping data collection for 3.917 Prior Living Situation.",
          "project_types_excluded": ["ES", "SERVICES_ONLY", "SH"],
          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "3.917.1",
              "operator": "EQUAL",
              "answer_group_code": "INSTITUTIONAL"
            },
            {
              "question": "3.917.2",
              "operator": "IN",
              "answer_codes": [
                "LOS_90_DAYS_OR_MORE_BUT_LESS_THAN_ONE_YEAR",
                "LOS_ONE_YEAR_OR_LONGER"
              ]
            }
          ]
        },
        {
          "type": "DISPLAY",
          "link_id": "3.917.B",
          "text": "Client stayed 7+ days in a transitional or permanent housing situation. This is considered a \"break\" according to the HUD definition of chronic homelessness. Stopping data collection for 3.917 Prior Living Situation.",
          "project_types_excluded": ["ES", "SERVICES_ONLY", "SH"],
          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "3.917.1",
              "operator": "EQUAL",
              "answer_group_code": "TEMPORARY_PERMANENT_OTHER"
            },
            {
              "question": "3.917.2",
              "operator": "IN",
              "answer_codes": [
                "LOS_ONE_WEEK_OR_MORE_BUT_LESS_THAN_ONE_MONTH",
                "LOS_ONE_MONTH_OR_MORE_BUT_LESS_THAN_90_DAYS",
                "LOS_90_DAYS_OR_MORE_BUT_LESS_THAN_ONE_YEAR",
                "LOS_ONE_YEAR_OR_LONGER"
              ]
            }
          ]
        },
        {
          "type": "BOOLEAN",
          "link_id": "3.917.C",
          "text": "On the night before, did the client stay on the streets, ES or SH?",
          "project_types_excluded": ["ES", "SERVICES_ONLY", "SH"],
          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "3.917.A",
              "operator": "ENABLED",
              "answer_boolean": false
            },
            {
              "question": "3.917.B",
              "operator": "ENABLED",
              "answer_boolean": false
            }
          ]
        },
        {
          "type": "DISPLAY",
          "link_id": "3.917.C.message",
          "text": "Stopping data collection for 3.917 Prior Living Situation.",
          "project_types_excluded": ["ES", "SERVICES_ONLY", "SH"],
          "_comment": "FIXME: If 2.917 B or C were enabled, we shouldnt show this unless B/C have been answered?",
          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "3.917.C",
              "operator": "EQUAL",
              "answer_boolean": false
            },
            {
              "question": "3.917.1",
              "operator": "EXISTS",
              "answer_boolean": true
            },
            {
              "question": "3.917.2",
              "operator": "EXISTS",
              "answer_boolean": true
            },
            {
              "question": "3.917.A",
              "operator": "ENABLED",
              "answer_boolean": false
            },
            {
              "question": "3.917.B",
              "operator": "ENABLED",
              "answer_boolean": false
            }
          ]
        },
        {
          "type": "DATE",
          "link_id": "3.917.3",
          "text": "Date homelessness started",
          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "3.917.A",
              "operator": "ENABLED",
              "answer_boolean": false
            },
            {
              "question": "3.917.B",
              "operator": "ENABLED",
              "answer_boolean": false
            },
            {
              "question": "3.917.C.message",
              "operator": "ENABLED",
              "answer_boolean": false
            }
          ]
        },
        {
          "type": "CHOICE",
          "link_id": "3.917.4",
          "text": "Number of times the client has been on the streets, in ES, or SH in the past three years",
          "pick_list_reference": "TimesHomelessPastThreeYears",
          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "3.917.A",
              "operator": "ENABLED",
              "answer_boolean": false
            },
            {
              "question": "3.917.B",
              "operator": "ENABLED",
              "answer_boolean": false
            },
            {
              "question": "3.917.C.message",
              "operator": "ENABLED",
              "answer_boolean": false
            }
          ]
        },
        {
          "type": "CHOICE",
          "link_id": "3.917.5",
          "text": "Total number of months homeless on the street, in ES, or SH in the past three years",
          "pick_list_reference": "MonthsHomelessPastThreeYears",
          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "3.917.A",
              "operator": "ENABLED",
              "answer_boolean": false
            },
            {
              "question": "3.917.B",
              "operator": "ENABLED",
              "answer_boolean": false
            },
            {
              "question": "3.917.C.message",
              "operator": "ENABLED",
              "answer_boolean": false
            }
          ]
        }
      ]
    },
