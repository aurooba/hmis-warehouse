    {
      "type": "GROUP",
      "link_id": "3.917-group",
      "text": "Prior Living Situation (3.917)",
      "data_collected_about": "HOH_AND_ADULTS",
      "reference_link": "https://files.hudexchange.info/resources/documents/FY-2022-HMIS-Data-Standards-Manual.pdf#page=104",
      "item": [
        {
          "type": "DISPLAY",
          "link_id": "3.917-instruction",
          "text": "Where did the client spend last night? Select the category that most closely matches the client's response."
        },
        {
          "type": "CHOICE",
          "required": false,
          "link_id": "3.917-1",
          "text": "Prior Living Situation",
          "pick_list_reference": "PRIOR_LIVING_SITUATION"
        },
        {
          "type": "CHOICE",
          "link_id": "3.917-2",
          "text": "Length of stay in prior living situation",
          "pick_list_reference": "LengthOfStay"
        },
        {
          "type": "GROUP",
          "link_id": "pls-conditionals",
          "item": [
            {
              "type": "BOOLEAN",
              "link_id": "3.917-A",
              "text": "Did the client stay less than 90 days?",
              "project_types_excluded": ["ES", "SERVICES_ONLY", "SH"],
              "enable_behavior": "ALL",
              "enable_when": [
                {
                  "question": "3.917-1",
                  "operator": "EQUAL",
                  "answer_group_label": "Institutional"
                }
              ]
            },
            {
              "type": "BOOLEAN",
              "link_id": "3.917-B",
              "text": "Did the client stay less than 7 nights?",
              "project_types_excluded": ["ES", "SERVICES_ONLY", "SH"],
              "enable_behavior": "ALL",
              "enable_when": [
                {
                  "question": "3.917-1",
                  "operator": "EQUAL",
                  "answer_group_label": "Temporary, Permanent, or Other"
                }
              ]
            },
            {
              "type": "BOOLEAN",
              "link_id": "3.917-C",
              "text": "On the night before, did the client stay on the streets, ES or SH?",
              "project_types_excluded": ["ES", "SERVICES_ONLY", "SH"],
              "enable_behavior": "ANY",
              "enable_when": [
                {
                  "question": "3.917-A",
                  "operator": "EQUAL",
                  "answer_boolean": true
                },
                {
                  "question": "3.917-B",
                  "operator": "EQUAL",
                  "answer_boolean": true
                }
              ]
            }
          ]
        },
        {
          "type": "DATE",
