{
  "name": "funder-edit",
  "item": [
    {
      "type": "DATE",
      "link_id": "project-start",
      "hidden": true,
      "initial": [
        {
          "value_local_constant": "$projectStartDate"
        }
      ]
    },
    {
      "type": "DATE",
      "link_id": "project-end",
      "hidden": true,
      "initial": [
        {
          "value_local_constant": "$projectEndDate"
        }
      ]
    },
    {
      "type": "GROUP",
      "link_id": "name",
      "item": [
        {
          "type": "CHOICE",
          "required": true,
          "link_id": "funder",
          "text": "Funding Source",
          "pick_list_reference": "FundingSource",
          "field_name": "funder"
        },
        {
          "type": "STRING",
          "required": true,
          "link_id": "other",
          "text": "Other Funder",
          "field_name": "otherFunder",
          "enable_behavior": "ALL",
          "enable_when": [
            {
              "question": "funder",
              "operator": "EQUAL",
              "answer_code": "LOCAL_OR_OTHER_FUNDING_SOURCE"
            }
          ]
        },
        {
          "type": "STRING",
          "required": true,
          "link_id": "grant-id",
          "text": "Grant Identifier",
          "field_name": "grantId"
        }
      ]
    },
    {
      "type": "GROUP",
      "link_id": "dates",
      "text": "Operating Period",
      "item": [
        {
          "link_id": "date-range-warning",
          "type": "DISPLAY",
          "component": "WARNING_ALERT",
          "text": "Date range does not overlap with the project operating period.",
          "enable_behavior": "ANY",
          "enable_when": [
            {
              "question": "end",
              "operator": "LESS_THAN",
              "compare_question": "project-start"
            },
            {
              "question": "start",
              "operator": "GREATER_THAN",
              "compare_question": "project-end"
            }
          ]
        },
        {
          "type": "DATE",
          "required": true,
          "link_id": "start",
          "text": "Start Date",
          "field_name": "startDate",
          "bounds": [
            {
              "type": "MAX",
              "question": "end"
            }
          ]
        },
        {
          "type": "DATE",
          "required": false,
          "link_id": "end",
          "text": "End Date",
          "field_name": "endDate",
          "bounds": [
            {
              "type": "MIN",
              "question": "start"
            }
          ]
        }
      ]
    }
  ]
}
