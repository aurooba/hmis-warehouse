= render 'breadcrumbs'
- content_for :title, @generator.title
%header.mb-3.d-flex
  %h1= content_for :title
  .ml-auto
    = link_to 'Generate New Full CAPER', new_hud_reports_caper_path, class: 'btn btn-secondary'

- if @show_recent
  .well
    .d-flex
      %h2 CAPER Report
      .ml-auto
        = link_to({format: :zip},{class: 'btn btn-primary'}) do
          %span.icon-download2
          Download Zip File

    = render 'questions'

= render 'hud_reports/year_tabs'
= render 'hud_reports/reports_wrapper'
