- default_coc_code = GrdaWarehouse::Config.get(:site_coc_codes)
= simple_form_for @filter, as: :filter, url: url do |f|
  - content_for :filters_col_full do
    .row
      .col-sm-4
        = f.input :project_ids, collection: @filter.project_options_for_select(user: current_user), as: :grouped_select, group_method: :last, input_html: {multiple: true, class: 'select2'},label: 'Projects'
      .col-sm-4
        = f.input :data_source_ids, collection: @filter.data_source_options_for_select(user: current_user), input_html: {multiple: true, class: 'select2'}, label: 'Data Sources'
      .col-sm-4
        = f.input :start, as: :date_picker, label: 'Start Date', required: true
    .row
      .col-sm-4
        = f.input :project_group_ids, collection: @filter.project_groups_options_for_select(user: current_user), input_html: {multiple: true, class: 'select2'},label: 'Project Groups'
      .col-sm-4
        = f.input :coc_codes, collection: @filter.coc_code_options_for_select(user: current_user), input_html: {multiple: true, class: 'select2'}, label: 'CoC Codes', hint: default_coc_code
      .col-sm-4
        = f.input :end, as: :date_picker, label: 'End Date', required: true
    .row
      .col-sm-4
        = f.input :project_type_codes, collection: @filter.available_residential_project_types, input_html: { multiple: true, class: 'select2' }

  - content_for :filter_actions do
    = f.button :submit, value: 'Queue Report'

  = render 'warehouse_reports/filters', f: f
