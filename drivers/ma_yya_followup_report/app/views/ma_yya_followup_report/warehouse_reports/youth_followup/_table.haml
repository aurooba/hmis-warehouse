.card
  %table.table.table-striped
    %thead
      %tr
        %th Client
        %th Engagement Date
        %th Last Seen
    %tbody
      - @report.clients.each do |client|
        %tr
          %td= link_to "#{client[:first_name]} #{client[:last_name]}", client_path(client[:id])
          %td= client[:engagement_date]
          %td= client[:last_seen] || client[:engagement_date]
