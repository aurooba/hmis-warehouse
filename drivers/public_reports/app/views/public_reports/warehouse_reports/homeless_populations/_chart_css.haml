:css
  /* CHARTS */
  .chart-pit, .chart-hoh-pit {
    min-height: 240px;
    max-height: 240px;
  }
  .chart-donut.chart {
    width: 240px;
    min-height: 240px;
    max-height: 240px;
  }
  .chart-bar.chart {

  }
  .bb-axis-y path, .bb-axis-x path {
    stroke: #cccccc;
  }
  .legend-wrapper {
    display: flex;
    flex-wrap: wrap;
  }
  .legend-wrapper-vertical {
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
    align-content: center;
  }
  .legend-wrapper-vertical .chart-legend-item-prs1 {
    display: flex;
    width: 100%;
    margin-right: 2rem;
    margin-bottom: 0.5rem;
    align-content: flex-end;
  }
  .legend-wrapper-horizontal .chart-legend-item-prs1 {
    display: flex;
    width: 40%;
    margin-right: 2rem;
    margin-bottom: 0.5rem;
  }
  .chart-legend-item-prs1 .chart-legend-item-swatch-prs1 {
    width: 20px;
    height: 20px;
    margin-right: 0.8rem;
  }
  .household-size-chart {
    font-size: 3em;
    text-align: center;
  }
  .chart-donut .bb-chart-arcs-title {
    font-size: 1rem;
  }
  .race-chart-category-first {
    margin-bottom: -1.25em;
    font-weight: 500;
    font-size: 0.8em;
  }
  .race-chart-category-last {
    margin-top: -3em;
    margin-bottom: 0.75em;
    font-weight: 500;
    font-size: 0.8em;
  }
  /* END CHARTS */

  /* SLIDER */
  input[type=range].jQuarters {
    width: 100%;
    margin: 2px 0;
    background-color: transparent;
    -webkit-appearance: none;
    border-radius: 50px;
  }
  input[type=range].jQuarters:focus {
    outline: none;
  }
  input[type=range].jQuarters::-webkit-slider-runnable-track {
    background: #e7eef4;
    border: 1px solid #cccccc;
    width: 100%;
    height: 16px;
    border-radius: 50px;
    cursor: pointer;
  }
  input[type=range].jQuarters::-webkit-slider-thumb {
    margin-top: -3px;
    width: 20px;
    height: 20px;
    background: #11427d;
    border: 0;
    border-radius: 50px;
    cursor: pointer;
    -webkit-appearance: none;
  }
  input[type=range].jQuarters:focus::-webkit-slider-runnable-track {
    background: #ffffff;
  }
  input[type=range].jQuarters::-moz-range-track {
    background: #e7eef4;
    border: 1px solid #cccccc;
    width: 100%;
    height: 16px;
    cursor: pointer;
  }
  input[type=range].jQuarters::-moz-range-thumb {
    width: 20px;
    height: 20px;
    background: #11427d;
    border: 0;
    border-radius: 50px;
    cursor: pointer;
  }
  input[type=range].jQuarters::-ms-track {
    background: transparent;
    border-color: transparent;
    border-width: 2px 0;
    color: transparent;
    width: 100%;
    height: 16px;
    cursor: pointer;
  }
  input[type=range].jQuarters::-ms-fill-lower {
    background: #c1d3e2;
    border: 1px solid #cccccc;
  }
  input[type=range].jQuarters::-ms-fill-upper {
    background: #e7eef4;
    border: 1px solid #cccccc;
  }
  input[type=range].jQuarters::-ms-thumb {
    width: 20px;
    height: 20px;
    background: #11427d;
    border: 0;
    border-radius: 50px;
    cursor: pointer;
    margin-top: 0px;
    /*Needed to keep the Edge thumb centred*/
  }
  input[type=range].jQuarters:focus::-ms-fill-lower {
    background: #e7eef4;
  }
  input[type=range].jQuarters:focus::-ms-fill-upper {
    background: #ffffff;
  }
  /*TODO: Use one of the selectors from https://stackoverflow.com/a/20541859/7077589 and figure out
  how to remove the virtical space around the range input in IE*/
  @supports (-ms-ime-align:auto) {
    /* Pre-Chromium Edge only styles, selector taken from hhttps://stackoverflow.com/a/32202953/7077589 */
    input[type=range].jQuarters {
      margin: 0;
      /*Edge starts the margin from the thumb, not the track as other browsers do*/
    }
  }
  .data-list-markers {
    justify-content: space-between;
  }
  .data-list-marker-wrapper {
    display: flex;
    align-items: end;
  }
  .data-list-marker-mark-wrapper {
    width: 100%;
  }
  .data-list-marker-mark {
    width: 2px;
    height: 10px;
    background-color: #CCC;
    margin: 0 auto;
  }
  .data-list-marker-mark.first {
    margin-left: 12%;
  }
  .data-list-marker-mark.last {
    margin-right: 12%;
  }
  /* END SLIDER */
  .w-30 {
    width: 30%;
    margin-right: 3%;
  }
  .w-50 {
    width: 45%;
    margin-right: 5%;
  }
